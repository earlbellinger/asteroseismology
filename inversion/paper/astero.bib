issn = {15384357 0004637X},
journal = {The Astrophysical Journal},
keywords = {abundances,fundamental parameters,low-mass,methods,methods: statistical,oscillations,solar-type,stars,stars: abundances,stars: fundamental parameters,stars: low-mass,stars: oscillations,stars: solar-type,statistical},
number = {1},
pages = {31},
publisher = {IOP Publishing},
title = {{Fundamental Parameters of Main-Sequence Stars in an Instant With Machine Learning}},
url = {http://stacks.iop.org/0004-637X/830/i=1/a=31?key=crossref.87243a2cf683a41d8fd6d5b8cc8d9be3},
volume = {830},
year = {2016}
